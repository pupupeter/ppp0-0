\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[1;32m    875\u001b[0m                 \u001b[0mioargs\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0mmode\u001b[0m\u001b[0;34m,\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n","\u001b[0;31mFileNotFoundError\u001b[0m: [Errno 2] No such file or directory: '觀光遊憩據點按縣市及遊憩據點交叉分析.csv'"]}],"source":["import pandas as pd\n","import json\n","import matplotlib.pyplot as plt\n","\n","# Step 1: 讀取 CSV 檔案並生成箱型圖\n","def analyze_tourist_data(csv_file):\n","    # 讀取 CSV 檔案\n","    data = pd.read_csv(csv_file)\n","\n","    # 假設資料格式為： 'County', 'Month', 'Visitors'\n","    # 確保 'Month' 為數字類型\n","    data['Month'] = pd.to_datetime(data['Month'], format='%m').dt.month\n","\n","    # 對四個縣市進行分組並加總觀光人數\n","    counties = data['County'].unique()\n","    monthly_data = {}\n","\n","    for county in counties:\n","        monthly_data[county] = data[data['County'] == county].groupby('Month')['Visitors'].sum()\n","\n","    # 生成箱型圖\n","    plt.figure(figsize=(10, 6))\n","    plt.boxplot([monthly_data[county] for county in counties], labels=counties)\n","    plt.title('Monthly Tourist Visitors by County')\n","    plt.xlabel('County')\n","    plt.ylabel('Number of Visitors')\n","    plt.grid(True)\n","\n","    # 標註 Max 和 Medium\n","    for i, county in enumerate(counties):\n","        max_val = monthly_data[county].max()\n","        median_val = monthly_data[county].median()\n","        plt.text(i + 1, max_val, f'Max: {max_val}', ha='center', va='bottom', color='red')\n","        plt.text(i + 1, median_val, f'Median: {median_val}', ha='center', va='bottom', color='blue')\n","\n","    plt.show()\n","\n","# Step 2: 讀取 JSON 檔案並檢索旅館\n","def find_hotels(json_file, city, special_requirement, lowest_price, ceiling_price):\n","    # 讀取 JSON 檔案\n","    with open(json_file, 'r', encoding='utf-8') as f:\n","        hotels = json.load(f)\n","\n","    # 檢索符合條件的旅館\n","    filtered_hotels = [\n","        hotel for hotel in hotels\n","        if hotel['PostalAddress']['Town'] == city\n","        and special_requirement in hotel['ServiceInfo']\n","        and lowest_price <= hotel['LowestPrice'] <= ceiling_price\n","    ]\n","\n","    # 輸出前 5 筆符合條件的旅館\n","    if filtered_hotels:\n","        print(\"符合條件的旅館：\")\n","        for hotel in filtered_hotels[:5]:\n","            print(pd.Series({\n","                'HotelName': hotel['HotelName'],\n","                'LowestPrice': hotel['LowestPrice'],\n","                'CeilingPrice': hotel['CeilingPrice'],\n","                'Description': hotel['Description'],\n","                'PostalAddress.Town': hotel['PostalAddress']['Town'],\n","                'PostalAddress': hotel['PostalAddress'],\n","                'StreetAddress': hotel['StreetAddress']\n","            }).to_string())\n","    else:\n","        print(\"沒有符合條件的旅館。\")\n","\n","# 執行程式\n","if __name__ == \"__main__\":\n","    csv_file = '觀光遊憩據點按縣市及遊憩據點交叉分析.csv'\n","    json_file = 'HotelList.json'\n","\n","    # 分析觀光人數數據\n","    analyze_tourist_data(csv_file)\n","\n","    # 讓使用者輸入條件\n","    city = input(\"請輸入縣市：\")\n","    special_requirement = input(\"請輸入特殊需求：\")\n","    lowest_price = float(input(\"請輸入最低價格：\"))\n","    ceiling_price = float(input(\"請輸入最高價格：\"))\n","\n","    # 查找旅館\n","    find_hotels(json_file, city, special_requirement, lowest_price, ceiling_price)"]}]}