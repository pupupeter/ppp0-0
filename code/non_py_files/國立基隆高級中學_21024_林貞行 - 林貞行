rocess the data for the box plot\n","def prepare_tourist_data(df):\n","    # Ensure the month column is in datetime format\n","    df['Month'] = pd.to_datetime(df['Month'], format='%Y-%m', errors='coerce').dt.month\n","    df['County'] = df['County'].astype('category')  # Convert County to categorical\n","    return df\n","\n","# Prepare the data\n","tourist_data = prepare_tourist_data(tourist_data)\n","\n","# Generate box plot for the four counties\n","plt.figure(figsize=(12, 6))\n","tourist_data.boxplot(column='VisitorCount', by=['County', 'Month'], grid=False)\n","\n","# Set plot title and labels\n","plt.title('Monthly Tourist Visitor Counts by County')\n","plt.suptitle('')\n","plt.xlabel('County and Month')\n","plt.ylabel('Visitor Count')\n","\n","# Show max and median values on the plot\n","for county in tourist_data['County'].cat.categories:\n","    for month in range(1, 13):\n","        max_value = tourist_data[(tourist_data['County'] == county) & (tourist_data['Month'] == month)]['VisitorCount'].max()\n","        median_value = tourist_data[(tourist_data['County'] == county) & (tourist_data['Month'] == month)]['VisitorCount'].median()\n","        plt.annotate(f'Max: {max_value}', xy=(month + (county.cat.codes[0] * 12), max_value),\n","                     textcoords=\"offset points\", xytext=(0,10), ha='center', fontsize=8)\n","        plt.annotate(f'Median: {median_value}', xy=(month + (county.cat.codes[0] * 12), median_value),\n","                     textcoords=\"offset points\", xytext=(0,10), ha='center', fontsize=8)\n","\n","plt.show()\n","\n","# Load the hotel data from JSON\n","with open('HotelList.json', 'r', encoding='utf-8') as f:\n","    hotel_data = json.load(f)\n","\n","# Function to search for hotels based on user input\n","def search_hotels(city, lowest_price, ceiling_price, special_service):\n","    filtered_hotels = []\n","\n","    for hotel in hotel_data:\n","        if (hotel['City'] == city and\n","            lowest_price <= hotel['LowestPrice'] <= ceiling_price and\n","            special_service in hotel['ServiceInfo']):\n","            filtered_hotels.append(hotel)\n","\n","    # Return the first 5 matching hotels\n","    return pd.DataFrame(filtered_hotels).head(5)\n","\n","# User input for hotel search\n","city_input = input(\"請輸入縣市: \")\n","lowest_price_input = float(input(\"請輸入最低價: \"))\n","ceiling_price_input = float(input(\"請輸入最高價: \"))\n","special_service_input = input(\"請輸入特殊需求: \")\n","\n","# Search for hotels and display results\n","matching_hotels = search_hotels(city_input, lowest_price_input, ceiling_price_input, special_service_input)\n","\n","if not matching_hotels.empty:\n","    print(matching_hotels[['HotelName', 'LowestPrice', 'CeilingPrice', 'Description', 'PostalAddress']['Town', 'StreetAddress']].to_string(index=False))\n","else:\n","    print(\"找不到符合條件的旅館。\")\n"]}]}