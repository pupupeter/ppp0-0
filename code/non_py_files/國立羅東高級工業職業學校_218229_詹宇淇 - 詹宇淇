            ha='center', va='bottom', fontsize=10, color='black')\n","    plt.text(i + 1, row['max'], f'Max: {int(row[\"max\"])}',\n","             ha='center', va='bottom', fontsize=10, color='blue')\n","\n","plt.tight_layout()\n","plt.savefig('001.jpg')\n","plt.show()\n"],"metadata":{"id":"KdowXa0NM_OZ","executionInfo":{"status":"ok","timestamp":1728919294275,"user_tz":-480,"elapsed":731,"user":{"displayName":"詹宇淇","userId":"17338061014686375322"}},"outputId":"afbb0afa-a72b-4362-944a-7dcf7f7ee8d7","colab":{"base_uri":"https://localhost:8080/","height":489}},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x600 with 0 Axes>"]},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import json\n","import pandas as pd\n","\n","with open('HotelList.json', 'r', encoding='utf-8-sig') as file:\n","    data = json.load(file)\n","\n","hotels_df = pd.DataFrame(data['Hotels'])\n","hotels_df['PostalAddress.Town'] = hotels_df['PostalAddress'].apply(lambda x: x['Town'] if isinstance(x, dict) else None)\n","hotels_df['PostalAddress.StreetAddress'] = hotels_df['PostalAddress'].apply(lambda x: x['StreetAddress'] if isinstance(x, dict) else None)\n","hotels_df['PostalAddress.City'] = hotels_df['PostalAddress'].apply(lambda x: x['City'] if isinstance(x, dict) else None)\n","\n","user_input_city = input(\"縣市: \")\n","matching_hotels = hotels_df[hotels_df['PostalAddress.City'] == user_input_city]\n","\n","if matching_hotels.empty:\n","    print(\"未找到符合地點的旅館/民宿\")\n","    exit()\n","\n","user_input_special_condition = input(\"特殊需求: \")\n","if user_input_special_condition.lower() and user_input_special_condition.lower() != '無':\n","    matching_hotels = matching_hotels[matching_hotels['ServiceInfo'].str.contains(user_input_special_condition, case=False, na=False)]\n","\n","    if matching_hotels.empty:\n","        print(\"未找到符合地點和特殊需求的旅館/民宿\")\n","        exit()\n","try:\n","    price_input = input(\"價位範圍（範圍: 1-20000）: \")\n","    price = int(price_input)\n","    if price < 1 or price > 20000:\n","        print(\"未找到符合預算的旅館/民宿\")\n","        exit()\n","    final_matching_hotels = matching_hotels[(matching_hotels['LowestPrice'] <= price) & (matching_hotels['CeilingPrice'] >= price)]\n","except ValueError:\n","    print(\"未找到符合預算的旅館/民宿\")\n","    exit()\n","\n","if not final_matching_hotels.empty:\n","    columns_to_display = ['HotelName', 'LowestPrice', 'CeilingPrice', 'Description', 'PostalAddress.Town', 'PostalAddress.StreetAddress']\n","    print(final_matching_hotels[columns_to_display].head(5).to_string(index=False))\n","else:\n","    print(\"未找到符合預算的旅館/民宿\")\n","\n"],"metadata":{"id":"35LmSic0nE4-"},"execution_count":null,"outputs":[]}]}